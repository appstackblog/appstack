DirectoryIndex index.php index.html

# Chặn truy cập trực tiếp ipa / plist
RewriteEngine On
RewriteCond %{REQUEST_URI} ^/app/fatx007-ios-turbo/(ipas-fat|plist-fat)/ [NC]
RewriteRule . - [F,L]

<FilesMatch "\.(ipa|plist)$">
  <IfModule mod_authz_core.c>
    Require all denied
  </IfModule>
  <IfModule !mod_authz_core.c>
    Order allow,deny
    Deny from all
  </IfModule>
</FilesMatch>
